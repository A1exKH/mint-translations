#: usr/lib/linuxmint/mintNanny/mintNanny.py:35
#: usr/lib/linuxmint/mintNanny/mintNanny.py:175
msgid "About"
msgstr "關於"

#: usr/lib/linuxmint/mintNanny/mintNanny.py:39
msgid "Domain blocker"
msgstr "網域封鎖器"

#: usr/lib/linuxmint/mintNanny/mintNanny.py:60
msgid "Please type the domain name you want to block"
msgstr "請輸入你要封鎖的網域名稱"

#: usr/lib/linuxmint/mintNanny/mintNanny.py:60
msgid "Domain name:"
msgstr "網域名稱："

#: usr/lib/linuxmint/mintNanny/mintNanny.py:70
msgid "Invalid Domain"
msgstr "無效的網域"

#: usr/lib/linuxmint/mintNanny/mintNanny.py:71
#, python-format
msgid "'%s' is not a valid domain name."
msgstr "'%s' 不是一個有效的網域名稱。"

#: usr/lib/linuxmint/mintNanny/mintNanny.py:72
msgid ""
"Domain names must start and end with a letter or a digit, and can only "
"contain letters, digits, dots and hyphens."
msgstr "網域名稱必須以字母或位數作開始和結束，並只能含有字母、位數、點號 (.) 和連字號 (-)。"

#: usr/lib/linuxmint/mintNanny/mintNanny.py:73
msgid "Example: my.number1domain.com"
msgstr "範例: my.number1domain.com"

#: usr/lib/linuxmint/mintNanny/mintNanny.py:129 generate_desktop_files:25
#: generate_desktop_files:39
msgid "Domain Blocker"
msgstr "網域封鎖器"

#: usr/lib/linuxmint/mintNanny/mintNanny.py:135
msgid "Blocked domains"
msgstr "已封鎖的網域"

#: usr/lib/linuxmint/mintNanny/mintNanny.py:163
msgid "_File"
msgstr "檔案(_F)"

#: usr/lib/linuxmint/mintNanny/mintNanny.py:167
msgid "Close"
msgstr "關閉"

#: usr/lib/linuxmint/mintNanny/mintNanny.py:171
msgid "_Help"
msgstr "協助(_H)"

#: generate_desktop_files:25 generate_desktop_files:39
msgid "Block access to selected domain names"
msgstr "封鎖對已選網域名稱的存取"

#: generate_desktop_files:39
msgid "Parental Control"
msgstr "家長控制"
